{
    "name"    : "flint/tacker",
    "description" : "Easy configuration based on Symfony Config",
    "license" : "MIT",

    "autoload" : {
        "psr-4" : { "Tacker\\" : "src" }
    },

    "require" : {
        "php"            : "^5.4|^7.0",
        "symfony/config" : "^2.7|^3.0"
    },

    "require-dev" : {
        "pimple/pimple" : "^3.0",
        "symfony/yaml"  : "^2.4|^3.0"
    },

    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}
